{
   "extends": "fastify-tsconfig",
   "compilerOptions": {
      "outDir": "dist/src",
      "sourceMap": true
   },
   "typedocOptions": {
      "entryPoints": [
         "src/"
      ],
      "out": "docs",
      "entryPointStrategy": "Expand",
      "plugin": [
         "typedoc-theme-hierarchy"
      ],
      "theme": "hierarchy",
      "name": "Remondis backend"
   },
   "include": [
      "src/**/*.ts"
   ]
}